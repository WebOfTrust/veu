{
  "name": "startstudio-mithril-template",
  "version": "0.1.0",
  "description": "StartStudio's Mithril Project Template.",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "format": "prettier --write 'src/**/*.{js,html,jsx,json,scss,css,md}'",
