    "polythene-mithril": "^1.7.5",
    "rxjs": "^7.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/preset-env": "^7.15.6",
    "babel-jest": "^27.2.4",
    "eslint": "^8.0.1",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "jest-transform-stub": "^2.0.0",
    "mithril-query": "^4.0.1",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.2",
    "sass": "^1.43.2"
